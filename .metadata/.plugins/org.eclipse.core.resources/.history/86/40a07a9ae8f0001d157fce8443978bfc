package entities;

public class Conta {
	
	public int numeroDaConta;
	public int agencia;
	public String titular;
	public double saldo;
	
	// Alt + Shift + S
	// generate Getters and Setter
	
	
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return numeroDaConta + " \n" +
				agencia + " \n" +
				titular + " \n" +
				saldo + " \n";
	}

}
