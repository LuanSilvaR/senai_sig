package principal;

import java.util.Scanner;

import entities.Conta;

public class Program {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		Conta conta = new Conta();
		
		System.out.println("Entre em com o número da conta: ");
		conta.numeroDaConta = sc.nextInt();
		
		System.out.println("Entre agencia: ");
		conta.agencia = sc.nextInt();
		sc.nextLine();
		
		System.out.println("Entre com o titular da conta:");
		conta.titular = sc.nextLine();
		
		System.out.println("Entre com o saldo");
		conta.saldo = sc.nextDouble();
		
		sc.close();
	}

}
